{
    "env": {
        "es6": true
    },
    "extends": ["standard", "eslint:recommended"],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 8
    },
    "rules": {
        "import/no-webpack-loader-syntax": "off",
        "no-console": "off",
        "declaration-colon-newline-after": "always-multi-line"
    }
}